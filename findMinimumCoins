#include <bits/stdc++.h> 
int findMinimumCoins(int amount) 
{
    // Write your code here
    vector<int>arr={1,2,5,10,20,50,100,500,1000};
    int ans=0;
    for(int i=arr.size()-1;i>=0;i--){
        while(amount>=arr[i]){
            amount-=arr[i];
            ans++;
        }
    }
    return ans;
}
